includes:
    - vendor/phpstan/phpstan-strict-rules/rules.neon
    - vendor/phpstan/phpstan-deprecation-rules/rules.neon

parameters:
    # You can currently choose from 10 levels (0 is the loosest and "max" is the strictest).
    level: max

    paths:
        - src/

    excludePaths:
        - vendor/

    # Ignored errors
    ignoreErrors:
        - '#^Parameter \#1 \$children of method MagicSunday\\Webtrees\\DescendantsChart\\Model\\Node\:\:setChildren\(\) expects (.*), (.*) given.$#'
